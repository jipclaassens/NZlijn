			attribute<bool>   Vijzelgracht_TA   (NL_grid/domain) := AccessibilityChangeAboveThreshold  && TreatmentArea_src/Vijzelgracht                                       && closer2vijzel_then_depijp        && !closer2rokin_then_vijzel; 
